    "gpii-pouch":           "git://github.com/the-t-in-rtf/gpii-pouchdb.git#f56fca6cf89c6092a37169504c44b0aedf12f580",
    "infusion":             "git://github.com/fluid-project/infusion.git#539c8dc6bdd2170944c0a4c58da52dd37fb098c0",
    "request":              "2.54.0",
    "grunt":                "~0.4.4",
    "grunt-contrib-jshint": "~0.9.0",
    "grunt-gpii":           "git://github.com/GPII/grunt-gpii.git#58ae055833f437a9e91baedb9f39a22c5fdd727f",
    "grunt-jsonlint":       "1.0.4",
    "grunt-shell":          "0.6.4"
  },
  "devDependencies": {
