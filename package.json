{
  "name": "gpii-pouchdb-lucene",
  "version": "0.1.0",
  "description": "A wrapper for couchdb-lucene to allow it to be required and used in tests.",
  "main": "index.js",
  "repository": "https://github.com/GPII/gpii-pouchdb-lucene/",
  "config": {
    "url": "https://github.com/rnewson/couchdb-lucene.git",
    "hash": "ce773de1becf8a400e0344a43cf6eea3134dcc63",
    "version": "1.0.2",
    "srcDir": "src/couchdb-lucene",
    "distDir": "dist"
  },
  "scripts": {
    "test":        "node tests/all-tests.js",
    "clone":       "rm -rf $npm_package_config_srcDir && git clone $npm_package_config_url $npm_package_config_srcDir",
    "checkout":    "cd $npm_package_config_srcDir && git checkout $npm_package_config_hash",
    "build":       "cd $npm_package_config_srcDir && mvn -Dmaven.test.skip=true",
    "postinstall": "grunt dedupe-infusion && npm run clone && npm run checkout && npm run build"
  },
